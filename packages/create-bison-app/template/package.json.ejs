{
  "name": "<%= name %>",
  "version": "0.1.0",
  "private": true,
<% if (host.name === 'heroku') { -%>
  "cacheDirectories": [".next/cache"],
<% } -%>
  "scripts": {
    "dev": "bison dev",
    "lint": "yarn eslint . --ext .ts,.tsx --fix --ignore-pattern tmp",
    "start": "next start -p $PORT",
    "test": "bison test --watch",
    "test:e2e": "bison test e2e",
    "watch": "bison watch",
  },
  "dependencies": {
    "@chakra-ui/core": "^1.0.0-rc.5",
    "@chakra-ui/theme": "^1.0.0-rc.5",
<% if (host.name === 'vercel') { -%>
    "apollo-server-micro": "^2.18.1",
<% } -%>
<% if (host.name === 'heroku') { -%>
    "apollo-server-express": "^2.18.1",
<% } -%>
    "framer-motion": "^2.9.4",
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.6",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
<% if (host.name === 'vercel') { -%>
    "@vercel/node": "^1.7.3",
<% } -%> 
    "bisonapp": "^<%= bisonVersion %>",
    "concurrently": "^5.2.0",
    "cypress": "^6.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-echobind": "^0.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-environment-node": "^26.1.0",
    "lint-staged": "^10.2.11",
    "pg": "^8.3.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "ts-node-dev": "^1.0.0",
    "typescript": "^3.9.7"
  },
  "bison": {
    "branches": {
      "staging": "<%= repo.stagingBranch %>",
      "production": "<%= repo.productionBranch %>"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn lint",
    "*.{graphql}": "prettier --write"
  }
}
